cd SideProject
npm init
...
npm install -g webpack

// This command installs all of the dependences required to use the Babel loader for Webpack.
npm install babel-loader babel-core babel-preset-es2015 babel-preset-stage-2 babel-preset-react --save-dev

npm install express --save

npm install react react-dom --save-dev

npm install style-loader css-loader sass-loader node-sass --save-dev

npm install babel-cli -g --save-dev